{{fm-field label='User Name'
  value=model.username
  errors=model.errors.username
  placeholder='Username'
}}
{{fm-field label='Password'
  value=model.password
  errors=model.errors.password
  placeholder='Password'
  type="password"
}}
{{fm-field label='Email'
  value=model.email
  errors=model.errors.email
  placeholder='email'
  type="email"
}}
